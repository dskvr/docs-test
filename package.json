{
  "name": "eosjs2",
  "version": "1.0.0",
  "description": "Talk to eos API",
  "main": "dist/index.js",
  "scripts": {
    "build": "sh ./scripts/build.sh",
    "serve": "sh ./scripts/serve.sh",
    "build-serve": "sh ./scripts/build-and-serve.sh"
  },
  "author": "block.one",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EOSIO/eosjs2.git"
  },
  "dependencies": {},
  "devDependencies": {
    "gitbook-cli": "2.3.2",
    "gitbook-plugin-page-toc": "^1.1.0",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typedoc": "0.12.0",
    "typedoc-plugin-external-module-name": "^1.1.3",
    "typedoc-plugin-markdown": "1.1.13",
    "typescript": "^2.9.1",
    "webpack": "^4.11.0",
    "webpack-cli": "^3.0.2"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(tsx?)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testRegex": "(/src/.*(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testEnvironment": "node"
  }
}
